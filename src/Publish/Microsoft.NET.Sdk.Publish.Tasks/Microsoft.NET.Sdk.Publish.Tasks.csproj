<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" />
  <PropertyGroup>
    <TargetFrameworks>net46</TargetFrameworks>
    <RuntimeIdentifier>win7-x86</RuntimeIdentifier>
    <AssemblyName>Microsoft.NET.Sdk.Publish.Tasks</AssemblyName>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <OutputPath>..\..\..\bin\Debug\</OutputPath>
    <IntermediateOutputPath>..\..\..\obj\Debug\</IntermediateOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <OutputPath>..\..\..\bin\Release\</OutputPath>
    <IntermediateOutputPath>..\..\..\obj\Release\</IntermediateOutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
    <Compile Include="..\Microsoft.NETCore.Sdk.Publish.Tasks\TransformWebConfig.cs">
      <Link>WebConfigTransform\TransformWebConfig.cs</Link>
    </Compile>
    <Compile Include="..\Microsoft.NETCore.Sdk.Publish.Tasks\WebConfigTransform.cs">
      <Link>WebConfigTransform\WebConfigTransform.cs</Link>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Sdk">
      <Version>1.0.0-alpha-20161104-2</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Build.Framework">
      <Version>15.1.298-preview5</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Build.Utilities.Core">
      <Version>15.1.298-preview5</Version>
    </PackageReference>
  </ItemGroup>
  
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.IO.Compression.FileSystem" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
</Project>